{
  "name": "typescript-jest-template",
  "author": "Ivo de Vries",
  "license": "ISC",
  "version": "1.0.0",
  "description": "Template for typescript module with jest",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
    "test-wip": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --watch --testNamePattern='@wip'",
    "test-watch": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --watch --detectOpenHandles",
    "build": "tsc"
  },
  "keywords": [],
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.8",
    "@types/spotify-web-api-node": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "tree-kill": "^1.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "aws-sdk": "^2.1483.0",
    "dotenv": "^16.3.1",
    "node-lame": "^1.3.2",
    "open": "^9.1.0",
    "openai": "^4.14.0",
    "spotify-web-api-node": "^5.0.2"
  }
}
